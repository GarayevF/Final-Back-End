@model Setting
@{
    ViewData["Title"] = "Create";
}

<div class="row">
    <div class="col-lg-12">
        <div class="row justify-content-lg-between">
            <div class="col-lg-12">
                <h1 class="h3 mb-4 text-gray-800">Tənzimləmə Yaradılması</h1>
            </div>
        </div>
    </div>
    <div class="col-lg-12">
        <form method="post">

            <div class="form-group">
                <label asp-for="Key"></label>
                <input asp-for="Key" class="form-control" />
                <span asp-validation-for="Key" class="text text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Value"></label>
                <input asp-for="Value" class="form-control" />
                <span asp-validation-for="Value" class="text text-danger"></span>
            </div>
            <button type="submit" class="btn btn-primary">Submit</button>
        </form>

    </div>
</div>
